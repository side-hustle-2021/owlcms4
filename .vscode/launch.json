    "version": "0.2.0",
    "configurations": [  
        
    
        {
            "name": "Launch GO file",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}"
        },
        {
            "type": "java",
            "name": "Debug (Launch) - Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-Main<owlcms>",
            "request": "launch",
            "mainClass": "app.owlcms.Main",
            "projectName": "owlcms",
            "env": {
                "REGISTER_API_TOKEN": "abcd",
                "DEFAULT_USER_PASSWORD": "1234"
            }
        },
        {
            "type": "java",
            "name": "Debug (Launch)-Speakers<owlcms>",
            "request": "launch",
            "mainClass": "app.owlcms.sound.Speakers",
            "projectName": "owlcms"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-ResourceWalker<owlcms>",
            "request": "launch",
            "mainClass": "app.owlcms.utils.ResourceWalker",
            "projectName": "owlcms"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-Main<relay>",
            "request": "launch",
            "mainClass": "app.owlcms.relay.Main",
            "projectName": "relay"
        }
    ]
}